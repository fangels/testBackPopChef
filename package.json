{
  "name": "back-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development npm run db:migrate && NODE_ENV=development nodemon ./app.js",
    "db:init": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "db:migrate": "npx sequelize db:migrate",
    "dev:db:reset": "NODE_ENV=development npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
  "_moduleAliases": {
    "@middlewares": "routes/middlewares",
    "@models": "database/models",
    "@services": "services",
    "@config": "config",
    "@features": "features",
    "@repositories": "database/repositories",
    "app": "app"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.4.0",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  }
}
